{
  "ecmaVersion": 8,
  "libs": [
    "jquery",
    "browser",
    "underscore",
    "react",
    "chai"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "node": {
