<%- include("header") %>
<div class="box" id="heading">
    <h1> <%= listTitle %> </h1>
</div>

<div class="box">

    <!-- <li><%# Item %> </li> -->
    <% for( let i = 0; i < arr.length; i++ ) { %>
    <div class="item">
        <input type="checkbox">
        <p><%= arr[i]%></p>
    </div>

    <% } %>

    <form class="item" action="/" method="POST">

        <input type="text" name="task" placeholder="Enter new task" autofocus="true" autocomplete="off">
        <button type="submit" name="button" value=<%= listTitle %>>+</button>

    </form>
</div>

<%- include("footer") %>